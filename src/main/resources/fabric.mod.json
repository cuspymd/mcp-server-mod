{
	"schemaVersion": 1,
	"id": "mcp-server-mod",
	"version": "${version}",
	"name": "MCP Server Mod",
	"description": "A Fabric mod that implements an MCP (Model Context Protocol) server, enabling AI assistants like Claude to interact with Minecraft through structured commands.",
	"authors": [
		"cuspymd"
	],
	"contact": {
		"homepage": "https://github.com/cuspymd/mcp-server-mod",
		"sources": "https://github.com/cuspymd/mcp-server-mod",
		"issues": "https://github.com/cuspymd/mcp-server-mod/issues"
	},
	"license": "CC0-1.0",
	"icon": "assets/mcp-server-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"cuspymd.mcp.mod.MCPServerMod"
		],
		"client": [
			"cuspymd.mcp.mod.MCPServerModClient"
		]
	},
	"mixins": [
		"mcp-server-mod.mixins.json",
		{
			"config": "mcp-server-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.7",
		"java": ">=21",
		"fabric-api": "*"
	}
}